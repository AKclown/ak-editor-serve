{
  "name": "ak-editor-serve",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node bin/www",
    "dev": "cross-env NODE_ENV=dev ./node_modules/.bin/nodemon bin/www",
    "prd-dev": "cross-env NODE_ENV=dev pm2 start bin/pm2-prd-dev.config.js",
    "prd": "pm2 start bin/www",
    "lint": "yarn run lint:es && yarn run lint:prettier",
    "lint:es": "eslint --ext .js,.jsx,.ts,.tsx ./ --cache",
    "lint:prettier": "prettier --list-different \"**/*.{css,less,js,jsx,ts,tsx,json,yml,yaml,md}\"",
    "fix": "yarn run fix:es && yarn run fix:prettier ",
    "fix:es": "eslint --ext .js,.jsx,.ts,.tsx ./ --cache --fix",
    "fix:prettier": "prettier --write \"**/*.{css,less,js,jsx,ts,tsx,json,yml,yaml,md}\""
  },
  "dependencies": {
    "ajv": "^6.12.5",
    "debug": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^4.0.3",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "koa2-cors": "^2.0.6",
    "mongoose": "^6.3.3",
    "mysql2": "^2.3.3",
    "pug": "^2.0.3",
    "redis": "^3.1.2",
    "require-all": "^3.0.0",
    "sequelize": "^6.19.1",
    "simple-git": "^3.7.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^1.19.1",
    "prettier": "^2.6.2"
  }
}
